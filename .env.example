# Скопируйте в .env и подставьте свои значения:
# cp .env.example .env   (Linux/Mac)
# copy .env.example .env (Windows)

# PostgreSQL. Для локального Docker используйте:
# DATABASE_URL=postgresql://talimjob:talimjob_pass@localhost:5432/talimjob
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DATABASE_NAME

# Опционально: сохранять вакансии также в JSON
SAVE_TO_JSON=false
